<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="lil" nsURI="http://www.crubier.com/lil/Lil" nsPrefix="lil">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="interactors" upperBound="-1"
        eType="#//InteractorDeclaration" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataTypes" upperBound="-1"
        eType="#//DataTypeCompoundDeclaration" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InteractorDeclaration">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="entities" upperBound="-1"
        eType="#//EntityDeclaration" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="signals" upperBound="-1"
        eType="#//SignalDeclaration" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="behaviors" upperBound="-1"
        eType="#//BehaviorDeclaration" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AccessibleEntity">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="generic" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="specific" eType="#//Entity"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//EntityDeclaration"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Actor">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//ActorDeclaration"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Component">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//ComponentDeclaration"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EntityDeclaration">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ActorDeclaration" eSuperTypes="#//EntityDeclaration"/>
  <eClassifiers xsi:type="ecore:EClass" name="ActorAlias">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//Actor"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="remote" eType="#//Actor"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ComponentDeclaration" eSuperTypes="#//EntityDeclaration">
    <eStructuralFeatures xsi:type="ecore:EReference" name="interactor" eType="#//InteractorDeclaration"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="aliases" upperBound="-1"
        eType="#//ActorAlias" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EventReception">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//EventDeclaration"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//Component"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="base" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FlowReception">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//FlowDeclaration"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//Component"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EventEmission">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//EventDeclaration"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destination" eType="#//Component"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FlowEmission">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="#//FlowDeclaration"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destination" eType="#//Component"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SignalDeclaration">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//DataType"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//AccessibleEntity"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destinations" upperBound="-1"
        eType="#//AccessibleEntity" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EventDeclaration" eSuperTypes="#//SignalDeclaration"/>
  <eClassifiers xsi:type="ecore:EClass" name="FlowDeclaration" eSuperTypes="#//SignalDeclaration"/>
  <eClassifiers xsi:type="ecore:EClass" name="BehaviorDeclaration">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cause" eType="#//Cause"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="effects" upperBound="-1"
        eType="#//Effect" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Cause">
    <eStructuralFeatures xsi:type="ecore:EReference" name="guard" eType="ecore:EClass ../../../org.eclipse.xtext.xbase/model/Xbase.ecore#//XExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OnCause" eSuperTypes="#//Cause">
    <eStructuralFeatures xsi:type="ecore:EReference" name="event" eType="#//EventReception"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WhenCause" eSuperTypes="#//Cause">
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="ecore:EClass ../../../org.eclipse.xtext.xbase/model/Xbase.ecore#//XExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Effect">
    <eStructuralFeatures xsi:type="ecore:EReference" name="value" eType="ecore:EClass ../../../org.eclipse.xtext.xbase/model/Xbase.ecore#//XExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AlwaysEffect" eSuperTypes="#//Effect">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//FlowEmission"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SetEffect" eSuperTypes="#//Effect">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//FlowEmission"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TriggerEffect" eSuperTypes="#//Effect">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//EventEmission"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataTypeCompoundDeclaration">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fields" upperBound="-1"
        eType="#//DataTypeCompoundField" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataTypeCompoundField">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//DataType"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataType"/>
  <eClassifiers xsi:type="ecore:EClass" name="DataTypeCompound" eSuperTypes="#//DataType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//DataTypeCompoundDeclaration"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataTypeBase" eSuperTypes="#//DataType">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
